  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.{js,jsx,vue,ts,tsx}' --fix",
    "prepare": "husky install"
  },
  "dependencies": {
